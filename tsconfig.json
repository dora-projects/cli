{
  "extends": "@tsconfig/node10/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "composite": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
  ]
}
