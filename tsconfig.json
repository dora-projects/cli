{
  "extends": "@tsconfig/node10/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "composite": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
